<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Access rights for x_hello_world model -->
        <record id="access_x_hello_world_user" model="ir.model.access">
            <field name="name">x_hello_world.user</field>
            <field name="model_id" ref="ir.model" search="[('model', '=', 'x_hello_world')]"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read">1</field>
            <field name="perm_write">1</field>
            <field name="perm_create">1</field>
            <field name="perm_unlink">1</field>
        </record>
    </data>
</odoo>